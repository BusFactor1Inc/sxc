-- handle lispy assignment and operations
    (= a b c ...)
    (+ a b c ...)

-- macros
   (macro (args ...) (code (CL)))

DONE -- add (struct name
     	  	    type1 field
		    type2 field ...)

-- Allow for nested struct definitions
   (struct point
   	   int x
	   int y
	   (struct data
	   	   int a
		   int b) d)